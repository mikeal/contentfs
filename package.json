{
  "name": "contentfs",
  "version": "0.0.0-development",
  "description": "A filesystem style tree on top of content-addressable data storage.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "commit": "git-cz",
    "test": "tap test/test-*.js --100",
    "posttest": "standard",
    "coverage": "tap test/test-*.js --coverage-report=lcov",
    "postcoverage": "codecov",
    "precommit": "npm test",
    "prepush": "npm test",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [],
  "author": "Mikeal Rogers <mikeal.rogers@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "codecov": "^2.2.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.3",
    "require-nocache": "^1.0.0",
    "require-uncached": "^1.0.3",
    "rimraf": "^2.6.1",
    "standard": "^10.0.2",
    "tap": "^10.7.0",
    "validate-commit-msg": "^2.13.0",
    "semantic-release": "^6.3.6"
  },
  "dependencies": {
    "lucass": "^1.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikeal/contentfs.git"
  }
}
